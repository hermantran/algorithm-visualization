extends layout

block content
  div.container
    div.row.header
      div.col-md-5
        h1= title
      div.col-md-7
        ul.nav.nav-pills.pull-right
          - var routes = ['Visualization', 'Benchmarking', 'Source'];
          - for (var i = 0; i < routes.length; ++i)
            li
              a(href="#/#{routes[i].toLowerCase()}"). #{routes[i]}
          
    hr
          
    ng:view
    
  script(src='js/lib/require.min.js', data-main='js/main')